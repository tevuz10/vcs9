CREATE OR REPLACE DATASOURCE DF dea
    ROUTE LOCAL 'LocalConnection' '${databases.admin.datasources.df.dea.ROUTE.LOCAL.PATH}' FILENAMEPATTERN = 'a(.*).csv'
