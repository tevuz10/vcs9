CREATE OR REPLACE WRAPPER DF dd_neww
    DATASOURCENAME=dd
    OUTPUTSCHEMA (
        "Column0" = 'Column0'
    );

CREATE OR REPLACE TABLE dd_neww I18N jp (
        "Column0":text
    )
    CACHE OFF
    TIMETOLIVEINCACHE DEFAULT
    ADD SEARCHMETHOD dd_neww(
        I18N jp
        CONSTRAINTS (
             ADD "Column0" NOS ZERO ()
        )
        OUTPUTLIST ("Column0"
        )
        WRAPPER (df dd_neww)
    );

