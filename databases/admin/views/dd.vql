CREATE OR REPLACE WRAPPER DF dd
    DATASOURCENAME=dd
    OUTPUTSCHEMA (
        "Column0" = 'Column0'
    );

CREATE OR REPLACE TABLE dd I18N jp (
        "Column0":text
    )
    CACHE OFF
    TIMETOLIVEINCACHE DEFAULT
    ADD SEARCHMETHOD dd(
        I18N jp
        CONSTRAINTS (
             ADD "Column0" NOS ZERO ()
        )
        OUTPUTLIST ("Column0"
        )
        WRAPPER (df dd)
    );

