# REQUIRES-PROPERTIES-FILE - # Do not remove this comment!
CREATE OR REPLACE DATASOURCE CUSTOM "ds_forms_t-forms1"
    FOLDER = '/test'
    CLASSNAME='com.denodo.vdb.contrib.wrapper.xls.ExcelWrapper';

CREATE OR REPLACE WRAPPER CUSTOM "ds_forms_t-forms1"
    FOLDER = '/test'
    DATASOURCENAME="ds_forms_t-forms1"
    PARAMETERS (
      'Type of file' = 'Excel 2007 or later (*.xlsx)',
      'File location' = ROUTE LOCAL 'LocalConnection' '${databases.admin.folder.test.views.custom.ds_forms_t-forms1.File location.ROUTE.LOCAL.PATH}',
      'Worksheet' = 'Sheet1',
      'Extract data from all sheets' = false,
      'Extract sheet name as a new column' = false,
      'Has headers' = true,
      'Stream tuples' = true
    )
    OUTPUTSCHEMA (
        "商品" = '商品' : 'java.lang.String' (OPT) (sourcetypeid='12')  SORTABLE,
        "金額" = '金額' : 'java.lang.Integer' (OPT) (sourcetypeid='4')  SORTABLE
    );

